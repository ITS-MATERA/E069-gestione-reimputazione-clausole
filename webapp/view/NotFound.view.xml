<mvc:View
	controllerName="funzionalitareimputazione.controller.NotFound"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
>
	<MessagePage
		title="{i18n>notFoundTitle}"
		text="{i18n>notFoundText}"
		icon="sap-icon://document"
		id="page"
		description=""
	>
		<customDescription>
			<Link
				id="link"
				text="{i18n>backToProvisions}"
				press=".onLinkPressed"
			/>
		</customDescription>
	</MessagePage>
</mvc:View>