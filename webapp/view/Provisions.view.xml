<mvc:View
	controllerName="funzionalitareimputazione.controller.Provisions"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns:f="sap.f"
>
	<f:DynamicPage
		showFooter="true"
		class="dynamicPageContent"
	>
		<f:title>
			<f:DynamicPageTitle/>
		</f:title>
		<f:header>
			<f:DynamicPageHeader>
				<core:Fragment
					fragmentName="funzionalitareimputazione.view.fragment.HeaderToolbar"
					type="XML"
				/>
			</f:DynamicPageHeader>
		</f:header>
		<f:content>
			<core:Fragment
				fragmentName="funzionalitareimputazione.view.fragment.TableProvisions"
				type="XML"
			/>
		</f:content>
		<f:footer>
			<OverflowToolbar>
				<ToolbarSpacer/>
				<Button
					id="btnRegisterReimputation"
					text="{i18n>btnRegisterReimputation}"
					type="Emphasized"
					press="onRegisterReimputation"
					visible="false"
					enabled="false"
				>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</Button>
			</OverflowToolbar>
		</f:footer>
	</f:DynamicPage>
</mvc:View>